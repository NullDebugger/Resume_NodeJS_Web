<div class="container mt-4">
  <div class="row">
    <div class="col-md-8">
      <article class="userslist maincontent">
        <h1 class="userslist-title"><%=information.title%></h1>
        <div class="userslist-name">
          of
          <a href="/users/<%=information.title%>"><%=information.name%></a>
        </div>
        <p class="userslist-info mt-2">
          <a href="/users/<%=information.title%>"> </a>
          <img
            class="userslist-img img-fluid"
            src="/images/titles/<%=information.title%>.jpg"
            alt="Photo of <%=information.name%> Post"
          />
        </p>
        <%-information.description %>
        <p></p>
      </article>
    </div>

    <aside class="col-md-4">
      <%- include('./partials/add_description') %> <%- include('./partials/artwork') %>
    </aside>
  </div>
</div>
